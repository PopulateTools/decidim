<div class="card" id="stats">
  <div class="card-divider">
    <h2 class="card-title">
      <%= t("participatory_process_stats.index.stats_title", scope: "decidim.admin") %>
    </h2>
  </div>

  <div class="card-section">
    <div class="table-scroll">
      <table class="table-list">
        <thead>
          <%= @process_stats.headers %>
        </thead>

        <tbody>
          <%= @process_stats.values %>
        </tbody>
      </table>
    </div>
  </div>
</div>
